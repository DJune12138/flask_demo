    // 活动类型转换
    var $recharge_money = '';
    var $present_money = '';
    var $max_present = '';
    var $journal_require = '';
    var $apply_times = '';
    var $add_change = '';
    var outermost_html = '';
    var change_class = '';
    function activity_type_change(){
        if($('#activity_type').val()==6) {
            $('#type_1').show();
            $('#type_2').hide();
            $('#water_tips1').show();
            $('#water_tips2').hide();
            $('#max_tips1').show();
            $('#max_tips2').hide();
            $recharge_money = $('.recharge_money_1');
            $present_money = $('.present_money_1');
            $max_present = $('.max_present_1');
            $journal_require = $('.journal_require_1');
            $apply_times = $('.apply_times_1');
            $add_change = $('#type_1');
            outermost_html = '<div class="change_1">';
            change_class = '.change_1';
        }
        else {
            $('#type_1').hide();
            $('#type_2').show();
            $('#water_tips1').hide();
            $('#water_tips2').show();
            $('#max_tips1').hide();
            $('#max_tips2').show();
            $recharge_money = $('.recharge_money_2');
            $present_money = $('.present_money_2');
            $max_present = $('.max_present_2');
            $journal_require = $('.journal_require_2');
            $apply_times = $('.apply_times_2');
            $add_change = $('#type_2');
            outermost_html = '<div class="change_2">';
            change_class = '.change_2'
        }
    }

    // 增加充值梯度点击事件
    $('#add_recharge').click(function() {
        $add_change.append(outermost_html + $add_change.find(change_class).eq(-1).html())
    });

    // 删除充值梯度点击事件
    $('#delete_recharge').click(function(){
        if($add_change.find(change_class).length>1) {
            $add_change.find(change_class).eq(-1).remove()
        }
        else {
            alert('就剩下一个了，还要删吗？')
        }
    });

    // 构造修改页的充值梯度数据
    function modification_recharge(data) {
        activity_type_change();
        var delete_length = $add_change.find(change_class).length-1;
        for (var i=0;i<delete_length;i++) {
            $('#delete_recharge').click()
        }
        for (var i=0;i<data.length-1;i++) {
            $('#add_recharge').click()
        }
        for (var i in data) {
            for (var j in data[i]) {
                $(change_class).eq(i).find('input').eq(j).val(data[i][j])
            }
        }
    }

    // 构造充值梯度数据
    function get_recharge() {
        var all_recharge_data = '[';
        $add_change.find(change_class).each(function() {
            var one = '[';
            $(this).find('input').each(function() {
                one = one + "'" + $(this).val() + "'" + ', '
            });
            one = one.substring(0,one.length - 2) + ']';
            all_recharge_data = all_recharge_data + one + ', '
        });
        all_recharge_data = all_recharge_data.substring(0,all_recharge_data.length - 2) + ']';
        return all_recharge_data
    }

    $(function () {
        $('#reportTable').bootstrapTable({
            striped: true,
            pagination: true,
            contentType: "application/x-www-form-urlencoded",
            paginationPreText:'上一页',
            paginationNextText:'下一页',
            showPaginationSwitch:false,  //是否显示数据条数选择框
            pageSize: 100,
            pageNumber:1,
            pageList: [10, 20, 50, 100, 200, 500],
            sidePagination:'client',
            showColumns: false,
            showRefresh: false,
            showHeader:true,
            showToolbar:false,
            buttonsAlign:"right",
            exportTypes: ["excel"],
            exportDataType: "all",
            clickToSelect: true,
            columns:
            [
                {#{field:"checked",checkbox:true},#}
                {field:"priority",title:"优先级",align:"center",sortable:true},
                {field:"activity_id",title:"活动ID",align:"center",valign:"middle"},
                // {field:"user_id",title:"后台账号ID",align:"center",valign:"middle"},
                {field:"activity_content",title:"活动规则描述",align:"center",valign:"middle"},
                {field:"activity_type",title:"活动类型",align:"center",valign:"middle"},
                {field:"participation_member",title:"参与玩家",align:"center",valign:"middle"},
                {field:"request_times",title:"申请次数",align:"center",valign:"middle"},
                {field:"max_add_recharge",title:"最高赠送",align:"center",valign:"middle"},
                {field:"journal_require",title:"流水要求",align:"center",valign:"middle"},
                {field:"begin_time",title:"开始时间",align:"center",valign:"middle"},
                {field:"end_time",title:"结束时间",align:"center",valign:"middle"},
                {field:"status",title:"状态",align:"center",valign:"middle"},
                {field:"operate",title:"操作",align:"center",valign:"middle"}
            ],
            data:[
                {% for one in data %}
                    {
                        priority:'{{ one.priority }}',
                        activity_id:'{{ one.activity_id }}',
                        // user_id:'{{ one.user_id }}',
                        activity_content:'{{ one.activity_content | safe }}',
                        activity_type:'{{ one.activity_type }}',
                        participation_member:'{{ one.participation_member }}',
                        request_times:'{{ one.request_times }}',
                        max_add_recharge:'{{ one.max_add_recharge/100 }}',
                        journal_require:'{{ one.journal_require }}',
                        begin_time:'{{ one.begin_time }}',
                        end_time:'{{ one.end_time }}',
                        status:'{% if one.status_num==0 %}<span style="color: red">{{ one.status }}</span>{% else %}<span style="color: limegreen">{{ one.status }}</span>{% endif %}',
                        operate:'<a activity_id="{{ one.activity_id }}" class="update_activity">修改</a>'
                    },
                {% endfor %}
            ]
        });

        // 构造修改活动页面
        $('.update_activity').click(function() {
            go_new.text('返回优惠列表');
            $('#batch_cancel').hide();
            $.ajax({
                url: '{{ url_for("busi.recharge_discounts_json") }}',
                type: 'GET',
                dataType: 'json',
                data: {
                    'activity_id': $(this).attr('activity_id')
                },
                success: function(data) {
                    $('.file_val1').val(data.data.activity_title);
                    $('.file_val2').val(data.data.show_picture_url);
                    $('.file_val3').val(data.data.tab1_url);
                    $('.file_val4').val(data.data.tab2_url);
                    $('#activity_type').val(data.data.activity_type);
                    $('#participation_member').val(data.data.participation_member);
                    $('#activity_content_create').val(data.data.activity_content);
                    modification_recharge(data.data.recharge_detail);
                    $('#journal_require').val(data.data.journal_require);
                    $('#apply_times').val(data.data.request_times);
                    $('#max_present').val(data.data.max_add_recharge);
                    $('#begin_time').val(data.data.begin_time);
                    $('#end_time').val(data.data.end_time);
                    $('#priority').val(data.data.priority);
                    {#$('#add-page').html('修改');#}
                    $('#tips').html('正在修改id为' + data.data.activity_id + '的活动');
                    $('.commit').attr('ONCLICK','update_activity()').attr('activity_id',data.data.activity_id);
                    getMemberLevel();
                    $('.hierarchy').removeClass('hierarchy-select');
                    data.data.participation_level.split(',').forEach(function (item) {
                        $('.hierarchy').each(function (idx, ite) {
                            if (item == $(ite).attr('item_id')) {
                                $(ite).addClass('hierarchy-select')
                            }
                        })
                    });
                    show_add();
                }
            })
        });

        {#// 活动通过审核#}
