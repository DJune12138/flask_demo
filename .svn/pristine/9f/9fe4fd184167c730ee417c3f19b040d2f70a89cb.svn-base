.bold {
    color: #000;
    font-weight: bold;
}

.big {
    width: 350px;
    position: fixed;
    bottom: 0px;
    left: 38%;
}

.min {
    margin: 0 auto;
    width: 100px;
    height: 25px;
    background: url("../images/search.png") no-repeat;
    overflow: hidden;
}

.max {
    margin: 0 auto;
    text-align: center;
    height: 40px;
    border-top: 1px solid #000;
