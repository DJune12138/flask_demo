}

.table_list td {
    padding: 4px 10px;
}

.table_list_head {
    background-color: #D7E4F5;
    color: #232323;
    font-weight: bold;
    text-align: center;
}

.table_normal {
    border-color: #DBDBDB;
    border-width: 1px;
    border-style: solid;
    width: 100%;
    line-height: 150%;
    border-spacing: 1px;
    border-collapse: collapse;
}

.table_normal td, .table_normal th {
    border: solid 1px #DBDBDB;
    padding: 4px 10px;
}

.tr_even {
    background-color: #EDEDED;
}

.tr_odd {
    background-color: #E2E4FF;
}

/*.operation, .update, .delete {*/
/*color: #5399ff !important;*/
/*}*/

ul, li {
    margin: 0;
    padding: 0;
    box-sizing: border-box;
}

li {
    list-style: none;
}

a, a:hover {
    text-decoration: none;
}

body {
    padding: 20px 20px !important;
    margin: 0 !important;
}

/*select-input样式*/
select, input {
    height: 32px !important;
    line-height: 1 !important;
    font-size: 12px !important;
    outline: none;
    border: 1px solid #e6e6e6 !important;
    background-color: #fff;
    padding: 0 5px;
    margin: 5px 0 !important;
    outline: none !important;
}

select {
    color: #8E8E8E !important;
    cursor: pointer;
}

input {
    background: none repeat scroll 0 0 #FFFFFF;
    text-indent: 5px;
}

input[disabled] {
    background: #ededed;
    cursor: no-drop !important;
}

.refactor_content p.content_item input[disabled] {
    height: 28px !important;
}

input[type='submit'], input[type='button'] {
    height: 32px !important;
    line-height: 1 !important;
    border-radius: 4px !important;
    margin-left: 10px !important;
    text-indent: 0px;
    border: none !important;
}

input[type='checkbox'], input[type='radio'] {
    position: relative;
    margin: 0 5px !important;
    left: 10px;
    top: 3px;
}

input:-webkit-autofill {
    -webkit-box-shadow: 0 0 0px 1000px #fff inset;
}

input:focus, select:focus {
    color: #000;
    border: 1px solid #0088ff !important;
}

input.cancel {
    border: solid 1px #c9c9c9 !important;
    background: #fff;
    color: #000;
}

input.cancel:hover {
    border: solid 1px #1e9fff !important;
    background: #fff;
    color: #000;
}

input.Wdate {
    width: 150px !important;
    font-size: 12px;
    border-radius: 4px !important;
}

/*table表格数据*/
table {
    border: solid 1px #ededed !important;
    width: 100%;
}

table tr th, table tr td {
    font-size: 12px;
    border: none;
    vertical-align: middle;
    padding: 10px;
    text-align: center;
}

table tr th {
    background: #f2f2f2;
}

table tr td a {
    color: #1890ff !important;
    cursor: pointer;
}

table tbody tr {
    border-bottom: solid #edf2f3 1px;
}

table tr td a:hover {
    text-decoration: none !important;
}

table tr:hover {
    background: #fafafa !important;
}

table tr:hover th {
    background: #f2f2f2 !important;
}

table tr:nth-child(even) {
    background: #f2f2f2;
}

table tr.no-records-found:hover td {
    background: #fff !important;
}

table tr:hover td {
    background: #e6f7ff !important;
}

table.nohover tr:hover td {
    background: #fff !important;
}

table tr th .sortable {
    user-select: none;
}

/*开关按钮*/
.switch_button {
    width: 60px;
    height: 22px;
    cursor: pointer;
    user-select: none;
    display: inline-block;
}

.switch_button[value="1"] {
    background-image: url(../images/button-open.png);
    background-repeat: no-repeat;
}

.switch_button[value="0"] {
    background-image: url(../images/button-close.png);
    background-repeat: no-repeat;
}

/*搜索查询栏*/
.search {
    text-align: center;
    padding: 10px;
    user-select: none;
    font-size: 12px;
}

.search > span {
    padding-left: 15px;
    font-size: 12px;
}

.search input, .search select {
    margin-right: 10px !important;
}

.search input {
    width: 80px !important;
}

.search input#query_btn {
    width: 60px !important;
}

.search input.Wdate {
    width: 165px !important;
}

.search span label {
    font-weight: normal;
}

.search input[name=endDate] {
    margin-left: 10px !important;
}

.search i {
    font-style: normal;
    margin: 0 10px 0 0;
}

/*日期快速查询*/
ul.date_fast {
    margin: 0 auto;
    overflow: hidden;
    display: inline-block;
    position: relative;
    top: 6px;
    float: right;
}

ul.date_fast li {
    font-size: 12px;
    float: left;
    border: solid #ededed 1px;
    border-right: none;
    padding: 7px 0;
    width: 40px;
    box-sizing: border-box;
    cursor: pointer;
}

ul.date_fast li:last-child {
    border-right: solid #ededed 1px;
}

ul.date_fast li.active {
    background: #1E9FFF;
    color: #fff;
}

/*新建-详情页面*/
.refactor_content {
    border: solid 1px #e6e6e6;
    border-radius: 5px;
}

.refactor_content .content_top {
    background: #f2f2f2;
    padding: 5px 2px;
    border-radius: 0px;
}

.refactor_content .content_item {
    margin: 10px 10px;
    border: 1px solid #e6e6e6;
    padding: 0;
}

.refactor_content .content_item_border_none {
    margin: 10px 10px;
    border: none;
    padding: 0;
}

.refactor_content .content_item label {
    margin: 0;
    background: #FBFBFB;
    width: 110px;
    height: 38px;
    border-right: 1px solid #e6e6e6;
    font-size: 13px;
    box-sizing: border-box;
    text-align: center;
    line-height: 38px;
}

.refactor_content .content_item input {
    width: calc(99% - 110px);
    border: none !important;
    height: 100% !important;
}

.refactor_content .content_item .switch_button {
    top: 8px;
}

.refactor_content .content_title {
    font-size: 13px;
    color: #666;
    margin: 8px 0 0 20px;
    display: inline-block;
}

.refactor_content .content_item input.radio {
    cursor: pointer;
    width: 13px;
    display: inline-block;
    position: relative;
    top: 5px;
    margin-left: 10px !important;
}

.refactor_content .content_item .radio:last-child {
    margin-left: 100px !important;
}

.refactor_content .content_item:last-child label {
    height: auto;
}

.refactor_content .content_item input[type='checkbox'] {
    width: 13px !important;
    height: 13px !important;
    border: solid 1px #000 !important;
    top: 0;
}

.refactor_content .content_item_last {
    position: relative;
}

.refactor_content .content_item_last .span {
    display: inline-block;
    padding: 5px 0 5px 120px;
}

.refactor_content .content_item_last span {
    display: block;
    font-size: 13px;
    margin: 3px 0;
}

.refactor_content .content_item_last label {
    height: 100%;
    display: inline-block;
    position: absolute;
    top: 0;
    bottom: 0;
}

.refactor_content .content_item_last label span {
    display: inline-block;
    position: absolute;
    left: 50%;
    top: 50%;
    transform: translate(-50%, -50%);
}

.refactor_content .btn-sm:focus {
    background: #1e9fff !important;
    border: none !important;
    outline: none !important;
}

.refactor_content .btn-sm:hover {
    background: #1e9fff !important;
}

.refactor_content_double .member_new {
    overflow: hidden;
    padding-top: 10px;
}

.refactor_content_double .content_item {
    width: 48% !important;
    float: left;
    margin: 10px 1% !important;
    box-sizing: border-box;
    padding-left: 115px !important;
    padding-right: 5px !important;
    position: relative;
    height: 40px;
}

.refactor_content_double .content_item label:first-child {
    position: absolute;
    left: 0;
}

.refactor_content_double .member_new .content_item input[type=text], .refactor_content_double .member_new .content_item select {
    width: 100% !important;
    box-sizing: border-box;
    height: 75% !important;
}

.refactor_content_double .content_item_nowarp {
    width: 98% !important;
}

.refactor_content_double .member_new {
    overflow: hidden;
}

/*提示信息普通弹窗*/
.refactor-alert {
    position: fixed;
    left: 0;
    top: 0;
    width: 100%;
    height: 100%;
    display: none;
    z-index: 99;
}

.refactor-alert .refactor-alert-content {
    width: 240px;
    background: #ec971f;
    position: absolute;
    left: 50%;
    top: 40%;
    box-sizing: border-box;
    padding: 15px 10px;
    text-align: center;
    color: #fff;
    font-size: 12px;
    border-radius: 3px;
    transform: translate(-50%, -50%);
}

/*二次确认弹窗*/
.refactor-confirm {
    position: fixed;
    left: 0;
    top: 0;
    width: 100%;
    height: 100%;
    background: rgba(0, 0, 0, .3);
    display: none;
}

.refactor-confirm .refactor-confirm-content {
    width: 240px;
    position: absolute;
    background: #fff;
    left: 50%;
    top: 40%;
    box-sizing: border-box;
    padding: 10px 10px;
    font-size: 12px;
    border-radius: 5px;
    transform: translate(-50%, -50%);
    text-align: center;
}

.refactor-confirm .refactor-confirm-text {
    margin: 20px 0;
}

.refactor-confirm .refactor-confirm-button {
}

